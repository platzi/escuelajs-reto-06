body
  background-color white
  margin 0
  padding 0

section
  margin 0
  padding 0

.btn
  border none
  background #93C541
  color #ffffff !important
  font-weight 300
  letter-spacing 1px
  padding 20px
  text-transform uppercase
  border-radius 6px
  display inline-block
  transition all 0.3s ease 0s
  position absolute
  z-index 3
  top 90%
  right 90%

.btn:hover
  color white !important
  font-weight 700 !important
  letter-spacing 3px
  background rgb(114, 168, 21)
  -webkit-box-shadow 0px 5px 40px -10px rgba(0,0,0,0.57)
  -moz-box-shadow 0px 5px 40px -10px rgba(0,0,0,0.57)
  transition all 0.3s ease 0s

.mapShown
  display block

.mapHidden
  display none

  
body
  background-color white
  margin 0
  padding 0
  font-size 16px

section
  margin 0
  paddin 0

.App
  width 100vw
  height 100vh

.btn
  padding .75rem 1.25rem
  border 0;
  border-radius 10px
  box-shadow 2px 2px 5px rgba(0,0,0,0.5)
  font-size 1rem
  font-weight bold
  text-transform uppercase
  cursor pointer


.btn-material
  position absolute
  top 1.5%
  right 6%
  width 200px
  z-index 100
  letter-spacing 1.5px
  color #FFFFFF
  background-color #bf360c

.upps {
  width 100%
  height 100vh
  font-size 2rem
  display flex
  justify-content center 
  align-items center
}